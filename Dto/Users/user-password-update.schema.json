{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "description": "Модель обновления пароля пользователя",
    "type": "object",
    "required": [
        "oldPassword",
        "newPassword"
    ],
    "properties": {
        "oldPassword": {
            "type": "string",
            "description": "Пароль пользователя",
            "nullable": false
        },
        "newPassword": {
            "type": "string",
            "description": "Новый пароль пользователя",
            "nullable": false
        }
    }
}